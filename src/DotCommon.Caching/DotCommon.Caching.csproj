  <Import Project="..\..\build\common.props" />
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    <AssemblyName>DotCommon.Caching</AssemblyName>
    <PackageId>DotCommon.Caching</PackageId>
